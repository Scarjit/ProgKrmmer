package main;

import java.io.*;
import java.util.*;

public class Main {

	public static void main(String[] args) {
		File tiere = new File("Daten\\Tiere.txt");
		Scanner input = null;
		try {
			input = new Scanner(
					new FileInputStream(tiere));
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		String line;
		while (input.hasNext()){
			line = input.nextLine();
			Zootier tier = Zootier.genZootier(line);
		}
		if (tier!=null)System.out.println(tier);
		

	}

}
