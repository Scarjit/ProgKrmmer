package beleg2;

public class SilberRing extends Ringe implements Juwelier{
	public SilberRing(){
		preis = new String[] {"100"};
		type = "Silber Ring";
	}
	@Override
	public String toString(){
		return "SilberRing: " + id + " kostet: " + getPreis();
	}
	@Override 
	public void kaufen() {
		System.out.println("Silber Ring: " + id + " wurde gekauft"); 
	}
	public String type(){
		return "Silber Ring";
	}
	public int id(){
		return id;
	}
}
