package secint;


public class Spielfeld {
	protected static int Größe;
	protected static PositionInterfae[][] Spielfeld;
	public static int getGröße() {
		return Größe;
	}
	public static void setGröße(int größe) {
		Größe = größe;
	}
	public static PositionInterfae[][] getSpielfeld() {
		return Spielfeld;
	}
	public static void setSpielfeld(PositionInterfae[][] spielfeld) {
		Spielfeld = spielfeld;
	}
	public static void genSpielfeld(int n){
		Spielfeld = new PositionInterfae[n][n];
	}
}
