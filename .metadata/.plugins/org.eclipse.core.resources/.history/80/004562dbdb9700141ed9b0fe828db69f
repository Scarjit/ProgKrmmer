package secint;

import java.util.Map;

public class Spielfeld {
	protected static int Größe;
	protected static Map<Integer,Item>[] Spielfeld;
	public static int getGröße() {
		return Größe;
	}
	public static void setGröße(int größe) {
		Größe = größe;
	}
	public static Map<Integer, Item>[] getSpielfeld() {
		return Spielfeld;
	}
	public static void setSpielfeld(Map<Integer, Item>[] spielfeld) {
		Spielfeld = spielfeld;
	}
}
