package beleg2;

public class AugenvonHypnos extends Schriftrollen implements Buchhandlung{
	public AugenvonHypnos(){
		preis = new String[] {"100"};
		type = "Augen von Hypnos";
	}
	@Override
	public String toString(){
		return "AugenvonHypnos: " + id + " kosten: " + getPreis();
	}
	@Override 
	public void kaufen() {
		System.out.println("Augen von Hypnos: "  + id + " wurde gekauft"); 
	}
	public String type(){
		return "Augen von Hypnos";
	}
	public int id(){
		return id;
	}
}
